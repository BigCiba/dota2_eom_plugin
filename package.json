{
	"name": "dota2-eom-plugin",
	"displayName": "Dota2月之底插件",
	"description": "Dota2月之底插件",
	"version": "1.0.0",
	"publisher": "bigciba",
	"engines": {
		"vscode": "^1.39.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"*",
		"onCommand:extension.SkinTool",
		"onCommand:extension.OpenLang"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.OpenLang",
				"title": "转到文本"
			},
			{
				"command": "extension.SkinTool",
				"title": "皮肤生成工具",
				"icon": {
					"light": "./images/skin_tool_light.svg",
					"dark": "./images/skin_tool_dark.svg"
				}
			}
		],
		"menus": {
			"editor/context": [
				{
					"when": "editorFocus",
					"command": "extension.OpenLang",
					"title": "转到文本",
					"group": "navigation"
				}
			],
			"editor/title": [
				{
					"command": "extension.SkinTool",
					"group": "navigation"
				}
			]
		},
		"configuration": {
			"type": "object",
			"title": "Dota2月之底插件",
			"properties": {
				"Dota2EomPlugin.addon_path": {
					"type": "string",
					"description": "项目路径"
				},
				"Dota2EomPlugin.text_url": {
					"type": "string",
					"description": "需求文本路径"
				}
			}
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.6",
		"@types/node": "^10.12.21",
		"@types/vscode": "^1.39.0",
		"glob": "^7.1.4",
		"mocha": "^6.1.4",
		"typescript": "^3.3.1",
		"tslint": "^5.12.1",
		"vscode-test": "^1.2.0"
	},
	"dependencies": {
		"@types/node-xlsx": "^0.12.1"
	}
}
